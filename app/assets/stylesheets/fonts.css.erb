@font-face {
    font-family: 'Open Sans';
    src: url(<%= font_path 'osans/opensans-light.eot' %>);
    src: url(<%= font_path 'osans/opensans-light.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= font_path 'osans/opensans-light.woff' %>) format('woff'),
         url(<%= font_path 'osans/opensans-light.ttf' %>) format('truetype');
    font-weight: 300;
    font-style: normal;

}



